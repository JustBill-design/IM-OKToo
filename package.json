{
  "name": "imoktoo",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:client\" \"npm run dev:server\"",
    "dev:client": "cd frontend && vite",
    "dev:server": "tsx watch backend/src/index.ts",
    "build": "cd frontend && vite build",
    "preview": "cd frontend && vite preview",
    "test": "npm run test:backend",
    "test:live": "npx tsx run-all-tests.ts",
    "test:live:api": "npx tsx test-cloud-connection.ts",
    "test:frontend": "vitest run",
    "test:frontend:watch": "vitest",
    "test:frontend:ui": "vitest --ui",
    "test:backend": "vitest run --config vitest.backend.config.ts",
    "test:backend:watch": "vitest --config vitest.backend.config.ts",
    "test:backend:unit": "vitest run --config vitest.backend.config.ts backend/src/tests/vitest-comprehensive.test.ts",
    "test:backend:integration": "vitest run --config vitest.backend.config.ts",
    "test:backend:live": "vitest run --config vitest.backend.config.ts",
    "test:backend:jest-broken": "echo 'Jest crypto issue pls use test:backend instead'",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:coverage": "npm run test:frontend -- --coverage && npm run test:backend -- --coverage",
    "test:all": "npm run test:frontend && npm run test:backend && npm run test:e2e",
    "migrate": "./run-migrations.sh",
    "migrate:cloud": "./run-migrations.sh",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:test": "docker-compose --profile testing up test-runner"
  },
  "dependencies": {
    "@google-cloud/cloud-sql-connector": "^1.8.2",
    "@internationalized/date": "^3.8.2",
    "@schedule-x/current-time": "^2.35.0",
    "@schedule-x/drag-and-drop": "^2.36.0",
    "@schedule-x/event-recurrence": "^2.36.0",
    "@schedule-x/events-service": "^2.36.0",
    "@schedule-x/resize": "^2.36.0",
    "@schedule-x/scroll-controller": "^2.35.0",
    "@schedule-x/theme-default": "^2.35.0",
    "@schedule-x/theme-shadcn": "^2.35.0",
    "@schedule-x/vue": "^2.33.0",
    "@tailwindcss/vite": "^4.1.11",
    "@vee-validate/zod": "^4.15.1",
    "@vueuse/core": "^13.6.0",
    "add": "^2.0.6",
