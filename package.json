    "date-fns": "^4.1.0",
    "dotenv": "^16.6.1",
    "embla-carousel-vue": "^8.6.0",
    "express": "^4.21.2",
    "googleapis": "^153.0.0",
    "hookable": "^5.5.3",
    "lucide-vue-next": "^0.525.0",
    "mysql": "^2.18.1",
