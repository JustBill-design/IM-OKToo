{
  "name": "imoktoo",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:client\" \"npm run dev:server\"",
    "dev:client": "cd frontend && vite",
    "dev:server": "tsx watch backend/src/index.ts",
    "build": "cd frontend && vite build",
    "preview": "cd frontend && vite preview",
    "test": "npm run test:frontend && npm run test:backend",
    "test:frontend": "cd frontend && vitest run",
    "test:frontend:watch": "cd frontend && vitest",
    "test:frontend:ui": "cd frontend && vitest --ui",
    "test:backend": "jest",
    "test:backend:watch": "jest --watch",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:coverage": "npm run test:frontend -- --coverage && npm run test:backend -- --coverage",
    "test:all": "npm run test && npm run test:e2e"
  },
  "dependencies": {
    "@internationalized/date": "^3.8.2",
    "@schedule-x/current-time": "^2.35.0",
    "@schedule-x/scroll-controller": "^2.35.0",
    "@schedule-x/theme-default": "^2.35.0",
    "@schedule-x/theme-shadcn": "^2.35.0",
    "@schedule-x/vue": "^2.33.0",
    "@tailwindcss/vite": "^4.1.11",
    "@vee-validate/zod": "^4.15.1",
    "@vueuse/core": "^13.5.0",
    "add": "^2.0.6",
    "button": "^1.1.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "hookable": "^5.5.3",
    "lucide-vue-next": "^0.525.0",
    "reka-ui": "^2.3.2",
    "shadcn-vue": "^2.2.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.11",
    "vee-validate": "^4.15.1",
    "vue": "^3.5.17",
    "vue-router": "^4.5.1",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@playwright/test": "^1.54.1",
    "@types/cors": "^2.8.19",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.23",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.0.10",
    "@types/supertest": "^6.0.3",
    "@vitejs/plugin-vue": "^6.0.0",
    "concurrently": "^9.2.0",
    "jest": "^30.0.4",
    "mysql2": "^3.14.2",
    "supertest": "^7.1.3",
    "ts-jest": "^29.4.0",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.20.3",
    "tw-animate-css": "^1.3.4",
    "typescript": "^5.8.3",
    "vite": "^7.0.0"
  }
}
